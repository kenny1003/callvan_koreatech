<div class = "num">
<h2>로그인하기</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :이메일주소 %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :비밀번호 %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "로그인" %>
  </div>
<% end %>

<!-- <%= render "devise/shared/links" %> -->

</div>
